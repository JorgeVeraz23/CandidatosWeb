{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "paths": {
      "src/*": [
        "./src/*"
      ]
    },
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/layouts/authentication/sign-in/illustration/index.tsx",
    "src/app/api/domain/repositories/ClienteRepository",
    "src/app/web.config.js",
    "src/routers/components/LoginRoutes.jsx",
    "src/components/Dashboard/ApptelinkDashboard.jsx"
, "src/app/api/domain/entities/CatalogQuestionItemEntity"  ],
}
